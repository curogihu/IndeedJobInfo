function toggleFixedSearch() {
var doc = document.documentElement;
var top = (window.pageYOffset || doc.scrollTop)  - (doc.clientTop || 0);
if (top > 34) {
addClass(document.body, "jobsearch-fixed");
} else {
removeClass(document.body, "jobsearch-fixed");
}
}
</script>
