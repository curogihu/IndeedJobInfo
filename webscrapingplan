<script type="text/javascript">
function showActiveJobAlertLabels( ) {
var label = gbid('tjobalerts');
if ( label ) {
label.innerHTML = '<div class="jobalertlabel"><a href="/my/alerts?q=php&amp;sort=date" class=member><span aria-label="alert icon" role="img" class="ico"></span> Job alerts active</a></div>';
}


var blabel = gbid('bjobalerts');
if ( blabel ) { blabel.parentNode.removeChild(blabel); }


}

if ( sendRequest ) {
sendRequest( '/rpc/jobalert?a=check&q=php&sort=date&tk=1adqjnkofbqkt8nr', 'POST', function( response ) {
if (response.readyState === 4) {
var responseJson = JSON.parse(response.responseText);
if (responseJson['status'] === 'active') {
showActiveJobAlertLabels();
}
}
}
);
}


var jobKeyToSave = getCookie('toSave');
if (jobKeyToSave !== '' && loggedIn === true) {
changeJobState(jobKeyToSave, 'save', 'linkbar');
delCookie('toSave');
}

restore('savedjobs',searchKey,searchState,window.tk,'');
</script>
<script type="text/javascript">
var jobCounts = {
'q=php&l=&sort=date' : 0,'q=php&l=' : 1,'q=php&l=Vancouver' : 2,'q=php&l=abc' : 3,'q=php&l=Vancouver%2C+BC' : 4,'q=Textnow%2C+Inc.&l=' : 5,'q=php&l=vancouver&sort=date' : 6,'q=Laravel&l=&sort=date' : 7,'q=php+aws&l=' : 8,'q=Magento&l=' : 9};

function pjcr_json(res) {
  var q = res.key;
  var row = jobCounts[q];
  if ( !isNaN(row) ) { 
    jobcount(row, q, res.newCount, res.newFormatted);
  }
}

function jobcount(row,q,nw,nwfmt) {
  var el = document.getElementById('rsl_'+row);
  if (!el || nw=='0') return;

  el.href += ( el.href.match(/\?/) ? '&' : '?' ) + "fromage=last";
  el.href = el.href.replace(/(\?|\&)sort=date&?/gi, '$1').replace(/[\?|\&]$/, '');

  var nji = document.createElement('span');
  nji.className = 'nji';
  nji.appendChild(document.createTextNode(nwfmt));

  var parent = el.parentNode;
  parent.insertBefore( nji, el.nextSibling );
  parent.insertBefore( document.createTextNode(' - '), el.nextSibling );
}</script>
